{
  "name": "sel-frontenf",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "watch-sass": "sass --watch sass/main.scss src/css/style.css",
